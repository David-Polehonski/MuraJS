<div class="{{{inputWrapperClass}}}" id="field-{{name}}-container">
	<div class="mura-checkbox-group">
		<div class="mura-group-label">
			{{#if summary}}{{summary}}{{else}}{{label}}{{/if}}{{#if isrequired}} <ins>Required</ins>{{/if}}</div>
			{{#if summary}}</br>{{/if}}
			{{#eachStatic dataset}}
				<div{{#if checkboxWrapperClass}} class="{{{checkboxWrapperClass}}}"{{/if}}>
					<input type="checkbox" name="{{../name}}" class="{{{checkboxClass}}}" id="field-{{datarecordid}}" value="{{value}}" {{#if isselected}} checked='checked'{{/if}}{{#if selected}} checked='checked'{{/if}}/>
					<label class="checkbox" class="{{{checkboxLabelClass}}}" for="field-{{datarecordid}}">{{label}}</label>
				</div>
			{{/eachStatic}}
	</div>
</div>
